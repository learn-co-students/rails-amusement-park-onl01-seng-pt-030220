<h1><%= @user.name %></h1>
<% if session[:admin] == false %>
    Height: <%= @user.height %><br>
    Mood: <%= @user.mood %><br>
    Tickets: <%= @user.tickets %><br>
<% else %>
    <h2>ADMIN</h2>
<% end %>

<% flash.each do |type, message| %>
    <%= message %>
<% end %>

<%= link_to "See attractions", attractions_path%>
<%= link_to "Log Out", logout_path %>
